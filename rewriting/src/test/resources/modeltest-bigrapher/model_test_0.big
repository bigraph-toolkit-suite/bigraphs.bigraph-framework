# Signature
ctrl Room = 1;
ctrl User = 1;
ctrl User2 = 0;
ctrl Spool = 1;
ctrl Printer = 2;
ctrl Computer = 1;
ctrl Job = 0;

big s1 = /e0 (
(Room{e0} . (Computer{b1} . (User{jeff2}.1 | Job.1)))
| (Room{e0} . (Computer{b1} . (User{jeff2}.1 | Job.1))) 
);

big r = (
(Computer{b1} . (User{jeff}.1 | Job.1)) 
|| (Computer{b1} . (User{jeff2}.1 | Job.1)) 
);

react combi2 = 
r -> r;

# Reactive system
begin brs
  init s1;
  rules = [{combi2}];
end
