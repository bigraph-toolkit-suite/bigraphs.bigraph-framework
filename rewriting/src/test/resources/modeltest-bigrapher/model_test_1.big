# Signature
ctrl Room = 1;
ctrl User = 1;
ctrl User2 = 0;
ctrl Spool = 1;
ctrl Printer = 2;
ctrl Computer = 1;
ctrl Job = 0;


big s1 = /e1 
((Room{e1} . (Computer{b1}.(Job.1) | User{jeff1}.1 ))
| 
(Room{e1} . (Computer{b1}.(Job.1 | User{jeff2}.1))));


big klo = (Computer{b1}.(Job.1) | User{jeff1}.1) || Computer{b1}.(Job.1 | User{jeff2}.1); 
react combi2 = klo -> klo;

# Reactive system
begin brs
  init s1;
  rules = [{combi2}];
end
