# Signature
ctrl Room = 1;
ctrl User = 1;
ctrl User2 = 0;
ctrl Spool = 1;
ctrl Printer = 2;
ctrl Computer = 1;
ctrl Job = 0;

big s1 = /e0 (
  (Room{e0} . (Computer{network}. ( User{a}.User{b}.1 | Job.Job.1 ) | User{bob}.1)) 
  | (Room{e0} . (User{jeff}.1))
);

big r = Computer{network} .(id(1) | Job.id(1));


react combi2 = 
r -> r;

# Reactive system
begin brs
  init s1;
  rules = [{combi2}];
end
